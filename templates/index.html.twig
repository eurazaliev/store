{# templates/index.html.twig #}
{% extends 'base.html.twig' %}

{% block body %}
<div class="blog-header">
    <h2 class="blog-title text-center">Main page</h2>
</div>
<div class="container">
<div class="col-6">
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>

<script src="//code.highcharts.com/4.1.8/highcharts.js"></script>
<script src="//code.highcharts.com/4.1.8/modules/exporting.js"></script>
<script type="text/javascript">
    {{ chart(memchart) }}
    {{ chart(cpuchart) }}
    {{ chart(oschart) }}
</script>
<div id="memchart" style="min-width: 300px; height: 300px; margin: 0 auto"></div>
<div id="cpuchart" style="min-width: 300px; height: 300px; margin: 0 auto"></div>
<div id="oschart" style="min-width: 300px; height: 300px; margin: 0 auto"></div>

<div class="list-group">
  <button type="button" class="list-group-item list-group-item-action active">
    It is demo application.
  </button>
  <button type="button" class="list-group-item list-group-item-action">
    Developed on PHP/Symfony 4/MariaDB.
  </button>
  <button type="button" class="list-group-item list-group-item-action">
    Demo data has been added using <a href=https://github.com/eurazaliev/store/tree/master/src/DataFixtures/ORM>fixtures</a>.
  </button>
  <button type="button" class="list-group-item list-group-item-action">
     bootstrap4/twig
  </button>
  <button type="button" class="list-group-item list-group-item-action">
    Acceptance test via Codeception.
  </button>
  <button type="button" class="list-group-item list-group-item-action">
    Source code <a href="https://github.com/eurazaliev/store">github тут</a>
  </button>
  <button type="button" class="list-group-item list-group-item-action">
    Search engine - elasticsearch.
  </button>
  <button type="button" class="list-group-item list-group-item-action">
    Application is running in docker.
  </button>

</div>
